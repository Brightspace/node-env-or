{
  "name": "env-or",
  "version": "1.0.2",
  "description": "Interface for process.env variables that allows setting a default value if none is set",
  "main": "src/index.js",
  "files": [
    "src",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "check-style": "eslint --ignore-path .gitignore .",
    "start": "node ./",
    "test": "npm run check-style && npm run test-no-style",
    "test-no-style": "export NODE_ENV=test; mocha ./test"
  },
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "homepage": "https://github.com/Brightspace/node-env-or",
  "repository": {
    "type": "git",
    "url": "https://github.com/Brightspace/node-env-or.git"
  },
  "bugs": {
    "url": "https://github.com/Brightspace/node-env-or/issues"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "eslint": "^8.15.0",
    "eslint-config-brightspace": "^0.17.0",
    "mocha": "^10.0.0",
    "sinon": "^14.0.0",
    "sinon-chai": "^3.7.0"
  },
  "dependencies": {
    "winston": "^1.0.1"
  },
  "eslintConfig": {
    "extends": "brightspace",
    "env": {
      "es6": true,
      "node": true
    }
  }
}
